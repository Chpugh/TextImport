Sub SumColumns()
    Dim col As Integer
    
    ' Loop through columns B to E (2 to 5)
    For col = 2 To 5
        ' Place the sum of rows 7 and 8 in row 10 for the current column
        Cells(10, col).Formula = "=SUM(" & Cells(7, col).Address & ":" & Cells(8, col).Address & ")"
    Next col
End Sub